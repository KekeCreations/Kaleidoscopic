{
    "schemaVersion": 1,
    "id": "kaleidoscopic",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "sources": "https://github.com/KekeCreations/Kaleidoscopic"
    },
    "license": "${license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.kekecreations.kaleidoscopic.FabricKaleidoscopic"
        ],
        "fabric-datagen": [
            "com.kekecreations.kaleidoscopic.datagen.KDataGenerator"
        ],
        "client" : [
            "com.kekecreations.kaleidoscopic.client.FabricKaleidoscopicClient"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=${java_version}",
        "jinxedlib": ">=1.0.3"
    },
    "suggests": {
    }
}
  